			"scope": [
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#6426",
				"fontStyle": ""
			}
		},
		{
			"name": "Bold Headings",
			"scope": [
				"strong",
				"markup.bold",
				"meta.selector",
				"markup.heading"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
