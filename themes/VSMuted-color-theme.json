	"tokenColors": [
		{
			"scope": [
				"comment",
				"constant.character",
				"constant.other.character-class",
				"emphasis",
				"markup.italic",
				"string",
				"support.constant.font-name"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#acacac"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.sha.git-rebase",
				"entity.name.function",
				"keyword.other.unit",
				"markup.inserted",
				"support.function",
				"token.info-token"
			],
			"settings": {
				"foreground": "#194"
			}
		},
		{
			"scope": [
				"constant.character.escape",
				"constant.regexp",
				"entity.name.tag",
				"invalid",
				"keyword.control.anchor.regexp",
				"markup.deleted",
				"markup.heading",
				"markup.inline.raw",
				"string.regexp",
				"token.error-token"
			],
			"settings": {
				"foreground": "#cc1184"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Definitions",
			"scope": [
				"entity.name.type.class",
				"meta.definition",
				"variable.parameter"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": [
				"punctuation.terminator.statement",
				"punctuation.separator.comma"
			],
			"settings": {
				"foreground": "#888"
			}
		},
		{
			"scope": [
				"markup.bold",
				"markup.heading",
				"meta.selector",
				"strong"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"constant.character",
				"constant.other.character-class",
				"string",
				"support.constant.font-name"
			],
			"settings": {
				"foreground": "#bb8000"
			}
		},
		{
			"name": "Keys",
			"scope": [
				"entity.other.attribute-name",
				"meta.object-literal.key",
				"meta.structure.dictionary.key",
				"support.type.property-name",
				"support.type.vendored.property-name",
				"variable.css",
				"variable.other.less",
				"variable.scss"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#1577b4"
			}
		},
		{
			"scope": [
				"markup.underline",
				"invalid",
				"token.error-token"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "#aaa"
			}
		},
		{
			"scope": [
				"punctuation.definition.template-expression",
				"punctuation.section.embedded"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#aaa"
			}
		},
		{
			"scope": [
				"meta.template.expression"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#222"
			}
		},
		{
			"scope": [
				"constant",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support"
			],
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"keyword"
			],
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"scope": [
				"keyword.control"
			],
			"settings": {
				"foreground": "#d52"
			}
		},
		{
			"scope": [
				"storage.type",
				"variable.language"
			],
			"settings": {
				"foreground": "#42e"
			}
		},
		{
			"scope": [
				"beginning.punctuation.definition.list.markdown",
				"beginning.punctuation.definition.quote.markdown",
				"constant",
				"entity.name.class",
				"entity.name.type",
				"entity.other.inherited-class",
				"markup.changed",
				"meta.return-type",
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"support.function.git-rebase",
				"support"
			],
			"settings": {
				"foreground": "#09a"
			}
		},
		{
			"scope": [
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color",
				"support.constant.media-type",
				"support.constant.media",
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#b70"
			}
		},
		{
			"scope": [
				"keyword.operator.regexp",
				"punctuation.character.set.begin.regexp",
				"punctuation.character.set.end.regexp",
				"punctuation.definition.character-class.regexp",
				"punctuation.definition.group.assertion.regexp",
				"punctuation.definition.group.regexp",
				"support.other.parenthesis.regexp"
			],
			"settings": {
				"foreground": "#222"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#ea1"
			}
		}
	],
	"type": "light"
}
