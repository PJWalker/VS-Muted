	"tokenColors": [
		{
			"scope": [
				"emphasis",
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#642"
			}
		},
		{
			"scope": [
				"meta.definition.variable.name"
			],
			"settings": {
				"foreground": "#62c"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"keyword.other.unit",
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"foreground": "#194"
			}
		},
		{
			"scope": [
				"constant.regexp",
				"markup.heading"
			],
			"settings": {
				"foreground": "#c15"
			}
		},
		{
			"name": "html tag names",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#c15",
				"fontStyle": ""
			}
		},
		{
			"name": "Definitions",
			"scope": [
				"meta.definition",
				"entity.name.type.class",
				"variable.parameter"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "subtle",
