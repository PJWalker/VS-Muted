		"terminal.ansiBlack": "#000",
		"terminal.foreground": "#222",
		"terminal.ansiRed": "#c54832",
		"terminal.ansiYellow": "#ad6c22",
		"terminal.ansiGreen": "#538020",
		"terminal.ansiBlue": "#4029b3",
		"terminal.ansiCyan": "#006d8b",
		"terminal.ansiMagenta": "#942d72"
